<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.lifeofmle.sophro.app" >

    <application
        android:allowBackup="true"
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".activities.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.lifeofmle.sophro.app.activities.SessionLimitActivity"
            android:label="@string/title_activity_session_limit" >
        </activity>
        <activity
            android:name="com.lifeofmle.sophro.app.activities.SessionActivity"
            android:label="@string/title_activity_session" >
        </activity>
    </application>

</manifest>
